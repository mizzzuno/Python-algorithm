   "source": [
    "memo = {1: 1, 2: 1}\n",
    "\n",
    "def fibonacci(n):\n",
    "    if n in memo:\n",
    "        return memo[n]\n",
    "    memo[n] = fibonacci(n - 2) + fibonacci(n - 1)\n",
    "    return memo[n]\n",
    "\n",
    "def main():\n",
    "    n = int(input('求めたいnを入力してください。')) \n",
    "    print(fibonacci(n))\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    main()"
   ]
  }
